(def defmacro (macro [sym params & body]
                     `(def ~sym (macro ~params ~@body))))

(defmacro defn [sym params & body]
  `(def ~sym (fn ~params ~@body)))

(defn debug-on []
  (print "Turning debugging on")
  (debug 1))
(defn debug-off []
  (print "Turning debugging off")
  (debug 0))

